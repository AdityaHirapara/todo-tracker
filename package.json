{
	"name": "todo-tracker",
	"displayName": "TODO Tracker",
	"description": "Handle your TODO tasks smartly, instead of lame comments",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "src/index.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.addTask",
				"title": "Add Task"
			},
			{
				"command": "extension.removeTask",
				"title": "Remove Task"
			},
			{
				"command": "extension.openTasks",
				"title": "Open Tasks Panel"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
